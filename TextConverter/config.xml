<?xml version="1.0" encoding="utf-8" ?>
<config>
  <input>
    <inputPathDirectory>C:\TextConverter\Input</inputPathDirectory>
    <inputFile>keyXmlExample.xml</inputFile>
    <inputFormat>separatedRows</inputFormat> <!-- separatedRows/singleLine IRRELEVANT FOR XML-->
    <splitKeyValue>=</splitKeyValue> <!-- The char that separate key from value IRRELEVANT FOR XML-->
    <splitVariables>,</splitVariables> <!-- The string that separate each variable IRRELEVANT FOR XML -->
    <firstValue>TimeStamp</firstValue>
    <lastValue>Value</lastValue>
  </input>
  <output>
    <outputPathDirectory>C:\TextConverter\Output</outputPathDirectory>
    <outputFile>output</outputFile>
    <outputType>JSON</outputType>
  </output>
  <settings>
    <runningOs>Windows</runningOs> <!-- Windows/Linux -->
    <refreshTime>5000</refreshTime> <!-- Time interval (ms) between input reads -->
  </settings>
</config>
